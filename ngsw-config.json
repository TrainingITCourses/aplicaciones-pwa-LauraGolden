{
  "index": "/index.html",
  "assetGroups": [
    {
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  },
  {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/icons/**"
      ],
      "urls":[
        "https://fonts.googleapis.com/icon?family=Material+Icons",
        "https://fonts.googleapis.com/css?family=Roboto:300,400,500"
      ]
    }
  }
],
  "dataGroups":[
    {
      "name": "cache-first",
      "urls":[
        "http://127.0.0.1:8080/assets/launchagencies.json",
        "http://127.0.0.1:8080/assets/launchlibrary.json",
        "http://127.0.0.1:8080/assets/launchmissions.json",
        "http://127.0.0.1:8080/assets/launchstatus.json"
      ],
      "cacheConfig":{
        "strategy":"performance",
        "maxSize":10,
        "maxAge":"7d"
      }
    },
    {
      "name": "cache-first",
      "urls":[
        
      ],
      "cacheConfig":{
        "strategy":"performance",
        "maxSize":2000,
        "maxAge":"1d"
      }
    },
    {
      "name": "api-first",
      "urls":[
        "https://launchlibrary.net/1.4/launch/1950-01-01?limit=2000"
      ],
      "cacheConfig":{
        "strategy" : "freshness",
        "maxSize": 2000,
        "maxAge" : "1d",
        "timeout" : "3s"
      }
    }
  ]
}
